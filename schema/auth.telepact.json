[
    {
        "///": [
            " The `@auth_` header is the conventional location for sending credentials to     ",
            " the server for the purpose of authentication and authorization.                 "
        ],
        "headers.Auth__": {
            "@auth_": "struct.Auth__"
        },
        "->": {}
    },
    {
        "struct.Auth__": {
            "token": "string"
        }
    },
    {
        "///": "Authenticate a user with username and password and receive an auth token",
        "fn.login": {
            "username": "string",
            "password": "string"
        },
        "->": [
            {
                "Ok_": {
                    "accessToken": "string"
                }
            },
            {
                "InvalidCredentials": {}
            }
        ]
    },
    {
        "///": "Retrieves a new access token with a valid refresh token",
        "fn.refresh": {
            "refreshToken": "string"
        },
        "->": [
            {
                "Ok_": {
                    "accessToken": "string"
                }
            },
            {
                "InvalidCredentials": {}
            }
        ]
    }
]